{
  "name": "saikyo-todo-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wrangler pages dev --compatibility-date=2023-03-01 --env dev --local --persist --d1=DB -- npm run dev:client",
    "dev:client": "vite",
    "build": " vite build",
    "deploy": "npm run build && wrangler pages publish pages",
    "preview": "vite preview",
    "test": "npx jest",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint --ext .tsx,.ts src/",
    "format": "prettier --write \"**/*.+(js|json|yml|ts|tsx)\"",
    "gen:component": "scaffdog generate component",
    "gen:page": "scaffdog generate page",
    "dev-db:init": "run-z dev-db:create-table dev-db:create-dummy-data",
    "dev-db:create-table": "wrangler d1 execute saikyo-todo-preview --file=./src/server/sql/create-table.sql",
    "dev-db:create-dummy-data": "wrangler d1 execute saikyo-todo-preview --file=./src/server/sql/create-dummy-data.sql"
  },
  "dependencies": {
    "hono": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@cloudflare/workers-types": "^4.20230228.0",
    "@hono/zod-validator": "^0.0.6",
    "@hookform/resolvers": "^2.9.11",
    "@storybook/addon-actions": "^7.0.0-beta.58",
    "@storybook/addon-essentials": "^7.0.0-beta.58",
    "@storybook/addon-interactions": "^7.0.0-beta.58",
    "@storybook/addon-links": "^7.0.0-beta.58",
    "@storybook/addon-mdx-gfm": "^7.0.0-beta.58",
    "@storybook/react": "^7.0.0-beta.58",
    "@storybook/react-vite": "^7.0.0-beta.58",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@tailwindcss/container-queries": "^0.1.0",
    "@tanstack/react-query": "^4.26.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "autoprefixer": "^10.4.13",
    "axios": "^1.3.4",
    "babel-loader": "^8.3.0",
    "class-variance-authority": "^0.4.0",
    "clsx": "^1.2.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "lucide-react": "^0.118.0",
    "msw": "^1.1.0",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prettier-plugin-tailwindcss": "^0.2.4",
    "react-hook-form": "^7.43.3",
    "run-z": "^1.11.0",
    "scaffdog": "^2.5.0",
    "storybook": "^7.0.0-beta.58",
    "tailwind-merge": "^1.10.0",
    "tailwindcss": "^3.2.7",
    "tailwindcss-animate": "^1.0.5",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-tsconfig-paths": "^4.0.5",
    "wrangler": "^2.9.1",
    "zod": "^3.20.6"
  }
}
